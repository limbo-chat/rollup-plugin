{
	"name": "rollup-plugin-limbo",
	"type": "module",
	"description": "Rollup plugin for developing Limbo plugins",
	"version": "0.0.2",
	"author": {
		"name": "Wes Lowe",
		"email": "wesley.jlowe10@gmail.com"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/limbo-chat/rollup-plugin"
	},
	"keywords": [
		"limbo",
		"llm",
		"ai"
	],
	"scripts": {
		"build": "tsup"
	},
